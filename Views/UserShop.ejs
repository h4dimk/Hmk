    <script>
      // Get references to the pagination divs
      const unfilteredPagination = document.getElementById(
        "unfilteredPagination"
      );
      const filteredPagination = document.getElementById("filteredPagination");

      // Check if a filter is applied based on URL parameters
      const urlParams = new URLSearchParams(window.location.search);
      const minPrice = urlParams.get("minPrice");
      const maxPrice = urlParams.get("maxPrice");

      // Show/hide the pagination controls based on filter presence
      if (!minPrice && !maxPrice) {
        // No filter applied, show unfiltered pagination
        unfilteredPagination.style.display = "block";
        filteredPagination.style.display = "none";
      } else {
        // Filter applied, show filtered pagination
        unfilteredPagination.style.display = "none";
        filteredPagination.style.display = "block";
      }
    </script>

