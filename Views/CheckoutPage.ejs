            <div class="menu-desktop">
              <ul class="main-menu">
                <li>
                  <a href="/user">Home</a>
                </li>
                <li>
                  <a href="/user/shop">Shop</a>
                </li>
                <li class="active-menu">
                  <a href="/user/cart">Cart</a>
